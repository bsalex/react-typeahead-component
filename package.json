{
  "name": "@bsalex/react-typeahead-component",
  "description": "Typeahead, written using the React.js library.",
  "author": "Ezequiel Rodriguez <ezequiel@yahoo.com>",
  "version": "0.11.1",
  "main": "./lib/index.js",
  "license": "MIT",
  "bugs": "https://github.com/ezequiel/react-typeahead-component/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ezequiel/react-typeahead-component.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typeahead",
    "autocomplete",
    "react-component",
    "component"
  ],
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run dist:dev && npm run dist:min",
    "build:lib": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "dist:dev": "webpack src/index.js -o dist/react-typeahead-component.dev.js --config webpack.config.development.js",
    "dist:min": "webpack src/index.js -o dist/react-typeahead-component.min.js --config webpack.config.production.js",
    "prepublish": "npm run clean && npm run build",
    "test": "karma start",
    "test:dev": "karma start --no-single-run --browsers Chrome"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "insert-css": "^2.0.0",
    "karma": "^2.0.4",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lolex": "^2.7.0",
    "mocha": "^5.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "react": "^16.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "babel-polyfill": "^6.26.0",
    "create-react-class": "^15.6.3"
  }
}
